
.colorBlock {
  height: 250px;
  background: -webkit-linear-gradient(to top, #5C95FC, #295BFD);
  /* Safari 5.1 - 6.0 */
  /* Opera 11.1 - 12.0 */
  /* Firefox 3.6 - 15 */
  background: -webkit-linear-gradient(bottom, #5C95FC, #295BFD);
  background: linear-gradient(to top, #5C95FC, #295BFD);
  /* 标准的语法 */
}
