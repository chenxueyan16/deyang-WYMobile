
.voteNumberBlock_item-number {
	width: 45px;
	height: 25px;
	background: -webkit-linear-gradient(left, #3558FF, #B65CFF);
	/* Safari 5.1 - 6.0 */
	/* Opera 11.1 - 12.0 */
	/* Firefox 3.6 - 15 */
	background: linear-gradient(left, #3558FF, #B65CFF);
	/* 标准的语法 */
	border-radius: 0px 100px 100px 0px;
	color: #fff;
	font-size: 12px;
	padding-left: 10px;
	box-sizing: border-box;
	line-height: 25px;
	position: absolute;
	-webkit-transform: translate(-5px, 5px);
	        transform: translate(-5px, 5px);
}
.prevent_img {
	height: 280px;
	width: 210px;
	margin: 0 auto;
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.vote_main_box {
	padding: 30px 16px 0 16px;
	overflow: hidden;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.vote_main_box>text:first-child {
	font-size: 14px;
	color: #333;
	line-height: 20px;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.vote_main_box>text:last-child {
	font-size: 12px;
	color: #FF4F4D;
	line-height: 20px;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	text-align: right
}

