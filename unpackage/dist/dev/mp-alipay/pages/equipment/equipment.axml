<view class="page"><u-navbar vue-id="fec1f710-1" back-icon-name="arrow-leftward" back-icon-color="#333" title="设备信息" border-bottom="{{false}}" onVueInit="__l"></u-navbar><view class="section1" style="padding-bottom:2px;"><view class="topbar"><text class="fl" style="color:#41424F;font-size:16px;font-weight:bold;margin-top:24px;">{{deptName}}</text></view><view style="margin:8px 16px;"><u-search vue-id="fec1f710-2" placeholder="请输入设备搜索关键字" shape="square" show-action="{{false}}" value="{{value}}" data-event-opts="{{[['^search',[['searchHandle']]],['^input',[['__set_model',['','value','$event',[]]]]]]}}" onSearch="__e" onInput="__e" onVueInit="__l"></u-search></view></view><block a:if="{{list.length==0}}"><u-empty style="margin-top:30px;" vue-id="fec1f710-3" text="暂无数据～" src="/static/img_nodata.png" iconSize="160" onVueInit="__l"></u-empty></block><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{list.length>0}}"><view class="equipmentBlock clearfix"><view class="fl equipmentBlock_item"><view>{{item.name}}</view><view>{{item.message}}</view></view><u-button class="fr" vue-id="{{'fec1f710-4-'+index}}" type="primary" size="mini" data-event-opts="{{[['^click',[['viewHandle',['$0',index],[[['list','',index]]]]]]]}}" onClick="__e" onVueInit="__l">查看状态</u-button><view class="fl" style="color:#888;width:100%;">{{item.code}}</view><view class="fl" style="color:#FF4F4D;width:100%;margin-top:2px;">{{item.warn}}</view></view></block></block></view>