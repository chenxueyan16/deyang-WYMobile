<view class="page"><u-navbar vue-id="11cdfa84-1" back-icon-name="arrow-leftward" back-icon-color="#333" title="投诉处理" border-bottom="{{false}}" custom-back="{{routerTo}}" onVueInit="__l"></u-navbar><view class="section1" style="padding-bottom:2px;"><view class="topbar"><text class="fl" style="color:#41424F;font-size:16px;font-weight:bold;margin-top:24px;">{{deptName}}</text></view><view class="dropDown"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="fl dropDown_item" onTap="__e"><text style="margin-right:10px;">{{status.text}}</text><u-icon vue-id="11cdfa84-2" size="10px" color="#41424F" name="arrow-down-fill" onVueInit="__l"></u-icon></view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="fl dropDown_item" onTap="__e"><text style="margin-right:10px;">{{type.text}}</text><u-icon vue-id="11cdfa84-3" size="10px" color="#41424F" name="arrow-down-fill" onVueInit="__l"></u-icon></view></view><u-action-sheet vue-id="11cdfa84-4" list="{{option1}}" cancel-btn="{{false}}" value="{{showStatus}}" data-event-opts="{{[['^click',[['changeStatus']]],['^input',[['__set_model',['','showStatus','$event',[]]]]]]}}" onClick="__e" onInput="__e" onVueInit="__l"></u-action-sheet><u-action-sheet vue-id="11cdfa84-5" list="{{option2}}" cancel-btn="{{false}}" value="{{showType}}" data-event-opts="{{[['^click',[['changeType']]],['^input',[['__set_model',['','showType','$event',[]]]]]]}}" onClick="__e" onInput="__e" onVueInit="__l"></u-action-sheet></view><block a:if="{{list.length==0}}"><u-empty style="margin-top:30px;" vue-id="11cdfa84-6" text="没有需要处理的信息～" src="/static/img_nodata.png" iconSize="160" onVueInit="__l"></u-empty></block><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{list.length>0}}"><view class="infoBlock" style="padding-right:0;"><view data-event-opts="{{[['tap',[['detailTo',['$0'],[[['list','',index]]]]]]]}}" class="clearfix" style="width:100%;" onTap="__e"><view class="infoBlock_text"><view class="clearfix infoBlock_text-item" style="width:100%;"><text>提交时间：</text><text>{{item.createDate}}</text><block a:if="{{item.state=='1'}}"><image class="fr infoBlock_img" src="/static/deal.png"></image></block><block a:if="{{item.state=='0'}}"><image class="fr infoBlock_img" src="/static/wait_deal.png"></image></block></view><view class="infoBlock_text-item"><text>提交人：</text><text class="overLong">{{item.userName}}</text></view><view class="infoBlock_text-item"><text>类型：</text><text>{{item.type}}</text></view></view></view><view class="overLong" style="width:100%;padding-right:15px;">{{item.text}}</view></view></block></block></view>